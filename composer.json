{
	"name": "sciactive/nymph-examples",
	"description": "Example apps built with Nymph.",
	"version": "1.0.0",
	"homepage": "http://nymph.io/",
	"authors": [
		{
			"name": "Hunter Perrin",
			"email": "hunter@sciactive.com",
			"homepage": "https://github.com/hperrin",
			"role": "Developer"
		}
	],
	"license": "LGPL",
	"require": {
		"sciactive/nymph-server": "dev-master",
		"sciactive/nymph-pubsub": "dev-master",
		"ext-pcntl": "*"
	},
	"scripts": {
		"post-install-cmd": [
			"$(npm bin -q)/bower install",
			"nohup php examples/pubsub.php -d > /dev/null 2>&1"
		],
		"post-update-cmd": [
			"$(npm bin -q)/bower install",
			"nohup php examples/pubsub.php -d > /dev/null 2>&1"
		]
	},
	"minimum-stability": "dev"
}
